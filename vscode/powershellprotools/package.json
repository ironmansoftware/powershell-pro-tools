{
    "name": "powershellprotools",
    "displayName": "PowerShell Pro Tools",
    "description": "Powerful extensions for PowerShell development.",
    "version": "2024.7.3",
    "publisher": "ironmansoftware",
    "repository": {
        "type": "git",
        "url": "https://github.com/ironmansoftware/powershell-pro-tools"
    },
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:powershell",
        "onLanguage:csharp",
        "onCommand:powershell.packageAsExe",
        "onCommand:poshProTools.openPsScriptPad",
        "onView:astView",
        "onView:hostProcessView",
        "onView:moduleView",
        "onView:providerView",
        "onView:quickScriptsView",
        "onView:variableView",
        "onView:infoView",
        "onView:reflectionView",
        "onView:customView",
        "onView:historyView",
        "onView:sessionsView",
        "onView:jobView",
        "onCommand:powershell.showWinFormDesigner",
        "onCommand:powershell.generateWinForm",
        "onCommand:poshProTools.statusBarMenu",
        "onCommand:poshProTools.installModule",
        "onCommand:poshProTools.welcome"
    ],
    "main": "./out/extension",
