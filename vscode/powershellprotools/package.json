            {
                "command": "jobView.stop",
                "title": "Stop",
                "icon": "$(debug-stop)"
            },
            {
                "command": "jobView.debug",
                "title": "Debug",
                "icon": "$(debug-alt)"
            },
            {
                "command": "jobView.remove",
                "title": "Remove",
                "icon": "$(close)"
            },
            {
                "command": "jobView.receive",
                "title": "Receive",
                "icon": "$(arrow-down)"
            },
            {
                "command": "jobView.refresh",
                "title": "Refresh",
